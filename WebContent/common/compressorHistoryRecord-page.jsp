<s:if test="oneIntegratedList.result.size!=0">
	<div>
	<a class="gt-image-button" href="javascript:jumpPage(1);" title="首页">
		<div class="gt-first-page" style="padding-top: 5px;"></div> 
	</a>
	<a class="gt-image-button <s:if test="!oneIntegratedList.hasPre">gt-button-disable</s:if>" 
