@charset "utf-8";
/* CSS Document */
body,td,th {
	font-family:Arial,'宋体',sans-serif;
	font-size: 12px;
	color: #333333;
}
img {
    border:none;
	}
body { 
    width:100%; 
	height:100%;
	margin-left:0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	background:url(../images/bg01.jpg) top repeat-x #d6defa;
	overflow-x:hidden;
}
ul {margin:0px; padding:0px;}
li {margin:0px; padding:0px; list-style-type:none;}
dl {margin:0px; padding:0px;}
dt {margin:0px; padding:0px;}
dd {margin:0px; padding:0px; list-style-type:none;}
#wrap:after {
    content: "."; 
    display: block;
    height: 0; 
    clear: both; 
    visibility: hidden;
}
a:link {
	color: #333333;
	text-decoration: none;
}
a:visited {
	color: #333333;
	text-decoration: none;
}
a:hover {
	color: #333333;
	text-decoration:none;
}
a:active {
	color: #333333;
	text-decoration: none;
}

/*顶层说明*/
.top_box {
	width:100%; height:auto; margin:0px; text-align:left; clear:both; overflow:hidden; background:url(../images/top_titbg.jpg) repeat-x top;
}
/*位置*/
.position_box {
	width:100%; height:38px; margin:0px; text-align:left; display:inline;
}
.pos_tit {
	float:left; padding-left:25px; background:url(../images/weizhi.gif) 5% 50% no-repeat; 
}
.sa_box {
	float:right; margin-right:10px; padding-top:6px;
}
.sa_input {
	width:50px; height:16px; text-align:center; line-height:16px; 
}
.position_box span {
	font-size:14px; font-weight:bold; font-family:‘微软雅黑’,‘黑体’,‘宋体’; line-height:38px; white-space:nowrap;
}
/*用户查询*/
.searlist_box {
	width:100%; height:40px; margin-top:8px;  border:#ddd487 1px solid; background:url(../images/sear_bg.jpg) repeat-x; padding-left:10px; *_padding-top:14px; padding-top:16px; display:inline-block; white-space:nowrap;
}
.searlist_box input {
	font-size:12px; line-height:14px; color:#333; width:auto; height:auto; 
}

/*用户信息表格*/
.userlist_box {
	width:100%; height:auto; margin-top:10px; clear:both; line-height:20px;
}
.page_box {
	background:url(../images/top_titbg.jpg) repeat-x top; height:30px; margin:0px; text-align:left; padding-top:8px; white-space:nowrap;
}
.choose_box {
	float:left; display:inline; margin-right:20px; padding-left:5px;
}
.page {
	float:right; display:inline; line-height:25px;
}
.count_box {
	padding-right:10px; text-align:right; text-indent:2em;float:right;
}
.userinfo_box {
	width:100%; height:auto; margin-top:5px; margin-bottom:5px; padding:0px;
}
.new_user {
	text-align:right; padding-right:10px; clear:both; height:35px; padding-top:8px;
}
/*列表标题*/
.userinfo_title {
	width:100%; height:30px; margin:0px; clear:both; text-align:left;  border:#D0E6EA 1px solid; background:#3A828E; display: block;
}
.userinfo_title span {
	color:#fff; font-weight:bold; line-height:30px; font-size:14px; 
}

