/*表单*/
.form_box {
	width:100%; height:auto; margin-top:10px; clear:both; line-height:20px; background-color:#F1F8F9; padding-top:15px; padding-bottom:15px; display:inline-block;
}
.form_con {
	width:90%; height:auto; margin:0px; display:inline; clear:both; white-space:nowrap;
}
.form_con ul {
	width:100%; height:34px; margin:0px; padding-top:7px; padding-left:5px; padding-right:5px; white-space:nowrap; border-bottom:#D0E6EA 1px dashed; clear:both;
}
.form_con_lil {
	width:20%; float:left; text-align:right; display:inline; white-space:nowrap;
}
.form_con_lir {
	width:20%; float:left; text-align:left; display:inline; white-space:nowrap;
}
.form_con_lir input {
	width:280px; text-align:left; line-height:18px; border:#008EAB 1px solid; height:18px;
}
.form_con_lir span {
	color:#E67817;
}
.move_box {
	width:90%; clear:both; height:auto; margin:0px; display:inline-block;
}
.present_user {
	width:20%; height:auto; margin:0px; float:left; display:inline-block; text-align:right;
}
.present_user p {
	line-height:18px; font-weight:bold; padding-top:15px; height:18px;
}
.move_icon {
	width:15%; height:auto; margin:0px; float:left; display:inline-block; text-align:center; padding-top:90px; padding-left:20px; padding-right:20px;
}
.system_user {
	width:auto; height:auto; margin:0px; float:left; display:inline-block; text-align:left;
}
.system_user p {
	line-height:18px; font-weight:bold; padding-top:15px; height:18px;
}






