@charset "utf-8";

/* CSS Document */
body {
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

body,td,th {
	font-family: Arial, '宋体', sans-serif;
	font: 12px;
}

img {
	border: none;
}

ul {
	margin: 0px;
	padding: 0px;
}

li {
	margin: 0px;
	padding: 0px;
	list-style-type: none;
}

dl {
	margin: 0px;
	padding: 0px;
}

dt {
	margin: 0px;
	padding: 0px;
}

dd {
	margin: 0px;
	padding: 0px;
	list-style-type: none;
}

#wrap:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}

a {
	font: 12px/1.5 tahoma, helvetica, clean, sans-serif;
	color: rgb(69, 100, 158);
	font-size-adjust: none;
	font-stretch: normal;
}

a:hover {
	text-decoration: none;
}

a:focus {
	outline-style: none;
}

/*个人信息*/
.user_info {
	 width:290px; height:auto; margin:0px; clear:both; overflow:hidden; display:block; position: absolute; z-index: 20;
}
.uinfo_co{
	width:278px; height:30px; background:url(../images/main_img01.png) 0px 0px no-repeat; text-align:left;
}
.uinfo_co ul {
	width:270px; height:auto; margin:0px auto;
}
.uinfo_co ul li {
	float:left; text-align:left; margin:0px; height:30px; line-height:30px; font-size:12px; color:#FFF; font-weight:bold;
}
.user_ico {
	background:url(../images/white_sicon.png) 0px 0px no-repeat; text-indent:2.5em;
}
.uinfo_co ul li span {
	font-size:12px; color:#FFFF99; line-height:30px; font-weight:lighter;
}
