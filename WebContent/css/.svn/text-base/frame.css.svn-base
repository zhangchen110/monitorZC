}*/

/*待办已办*/
.temp_wati {
	 width:290px; height:auto; margin-top:36px; clear:both; overflow:hidden; display:block; position: absolute; z-index: 20;
}
.temp_wati_co{
	width:278px; height:40px; background:url(../images/main_img01.png) 0px -36px no-repeat; text-align:left;
}
.temp_wati_co ul {
	width:270px; height:auto; margin:0px auto;
}
.temp_wati_co ul li {
	float:left; text-align:left; margin:0px; height:30px;
}
.templi1 {
	 background:url(../images/white_sicon.png) 1px -30px no-repeat; text-indent:2.2em; width:103px; 
}
.templi2 {
	 background:url(../images/white_sicon.png) 5px -60px no-repeat; text-indent:2.4em; width:103px;
}
.templiaro {
	background:url(../images/aro_b01.png) 0px 0px no-repeat; width:30px;
}
.templiaro img {
	width:30px; height:30px;
}
.temp_wati_co ul li span {
	font-size:12px; font-family:Verdana, Geneva, sans-serif, '宋体'; font-weight:bold; line-height:30px;
